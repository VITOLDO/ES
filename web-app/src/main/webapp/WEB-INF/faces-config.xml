<?xml version="1.0" encoding="UTF-8"?>

<faces-config 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>TradingOrderBean</managed-bean-name>
		<managed-bean-class>TradingOrderBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registrationBean</managed-bean-name>
		<managed-bean-class>RegistrationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rates</managed-bean-name>
		<managed-bean-class>RatesManagedBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>AccountBean</managed-bean-name>
		<managed-bean-class>AccountManageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<description>View information for all orders</description>
		<managed-bean-name>ordersHistoryBean</managed-bean-name>
		<managed-bean-class>OrdersHistoryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>changePass</managed-bean-name>
		<managed-bean-class>ChangePassBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>Logout</managed-bean-name>
		<managed-bean-class>LogoutBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userManageBean</managed-bean-name>
		<managed-bean-class>UserManageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>eventBean</managed-bean-name>
		<managed-bean-class>EventManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<converter>
		<converter-id>DecimalConverter</converter-id>
		<converter-class>DecimalConverter</converter-class>
	</converter>

</faces-config>
